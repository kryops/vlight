  "scripts": {
    "all": "yarn lint && yarn test && yarn build",
    "clean": "rimraf backend/dist frontend/dist",
    "build": "yarn run clean && yarn build:backend && yarn build:frontend",
    "build:backend": "tsc -p ./backend/src",
    "build:frontend": "cross-env NODE_ENV=production && webpack --config frontend/webpack.config.ts --env.production",
    "client:tcp": "ts-node backend/clients/vlight-tcp.ts",
    "client:udp": "ts-node backend/clients/vlight-udp.ts",
    "frontend:analyze": "webpack --config frontend/webpack.config.ts --env.production --env.analyze",
    "lint": "tslint --project . && tsc -p ./frontend && tsc -p ./backend",
    "postinstall": "rimraf node_modules/.cache",
    "prepare": "yarn build",
    "start": "nodemon",
    "start:prod": "cross-env NODE_ENV=production && node .",
    "test": "jest"
  },
